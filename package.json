{
  "name": "node-web",
  "version": "2.0.0",
  "description": "Template project for a node express web server",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/node-web"
  },
  "scripts": {
    "prettier:all": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.scss\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "build": "bash ./build.sh prod /node",
    "build-dev": "bash ./build.sh dev /node",
    "start": "bash -c 'NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "lint": "eslint \"public/**/*.{js,jsx}\" --quiet"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "applicationinsights": "^1.8.9",
    "body-parser": "^1.19.0",
    "component-registry": "^1.3.1",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "kth-node-access-log": "^0.2.2",
    "kth-node-api-call": "^3.0.12",
    "kth-node-configuration": "^2.0.3",
    "kth-node-express-routing": "^2.0.4",
    "kth-node-i18n": "^1.0.3",
    "kth-node-ldap": "^4.0.0",
    "kth-node-log": "^3.1.3",
    "kth-node-monitor": "^1.1.9",
    "kth-node-passport-cas": "^3.1.14",
    "kth-node-redis": "^3.1.7",
    "kth-node-server": "^3.1.2",
    "kth-node-session": "^1.0.4",
    "kth-node-web-common": "^5.1.8",
    "kth-style": "^6.0.7",
    "mobx": "^6.1.5",
    "mobx-react": "^7.1.0",
    "openid-client": "^4.4.2",
    "passport": "^0.4.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@kth/eslint-config-kth": "^1.0.9",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "concurrently": "^5.3.0",
    "eslint": "^7.19.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.7",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "sass": "^1.32.7"
  },
  "// (prettier)": "configuration version 2020-06-15 - might also reside in .prettierrc.json",
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "// (babel)": "configuration version 2020-10-21 (WEB) - might also reside in .babelrc.json",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "// (jest)": "configuration version 2020-06-15 (WEB) - might also reside as module.exports in jest.config.js",
  "jest": {
    "clearMocks": true,
    "notifyMode": "failure-change",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "verbose": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test"
    }
  }
}
