{
  "name": "node-web",
  "version": "2.0.0",
  "description": "Template project for a node express web server",
  "private": true,
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/node-web"
  },
  "scripts": {
    "prettier:all": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.scss\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "lint": "eslint \"{public,server}/**/*.{js,jsx}\" --quiet",
    "lint-v": "eslint \"{public,server}/**/*.{js,jsx}\" ",
    "prepare": "husky install",
    "postinstall": "bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@kth/api-call": "^4.0.37",
    "@kth/kth-node-passport-oidc": "^4.1.7",
    "@kth/kth-node-web-common": "^7.0.10",
    "@kth/log": "^4.0.4",
    "@kth/monitor": "^3.0.24",
    "@kth/server": "^4.0.2",
    "@kth/session": "^3.0.3",
    "applicationinsights": "^2.3.1",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-handlebars": "^6.0.5",
    "kth-node-access-log": "^0.2.8",
    "kth-node-configuration": "^2.0.11",
    "kth-node-express-routing": "^2.0.13",
    "kth-node-i18n": "^1.0.13",
    "kth-node-redis": "^3.1.40",
    "kth-style": "^8.0.26",
    "passport": "^0.5.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@kth/eslint-config-kth": "^3.0.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^27.4.1",
    "ansi-regex": "^6.0.1",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.1.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.14.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon": "^2.0.15",
    "null-loader": "^4.0.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "jest": {
    "clearMocks": true,
    "notifyMode": "failure-change",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "testEnvironment": "jsdom",
    "verbose": true
  }
}
