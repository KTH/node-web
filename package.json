{
  "name": "node-web",
  "version": "2.0.0",
  "description": "Template project for a node express web server",
  "private": true,
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/node-web"
  },
  "scripts": {
    "prettier:all": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.scss\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "lint": "eslint \"public/**/*.{js,jsx}\" --quiet",
    "prepare": "husky install",
    "postinstall": "bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "@kth/kth-node-monitor": "^2.0.0",
    "@kth/kth-node-passport-oidc": "^4.1.5",
    "@kth/kth-node-web-common": "^7.0.2",
    "@kth/log": "^4.0.0",
    "applicationinsights": "^2.2.0",
    "body-parser": "^1.19.1",
    "component-registry": "^1.3.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-handlebars": "^5.3.5",
    "kth-node-access-log": "^0.2.7",
    "kth-node-api-call": "^3.0.18",
    "kth-node-configuration": "^2.0.8",
    "kth-node-express-routing": "^2.0.12",
    "kth-node-i18n": "^1.0.12",
    "kth-node-redis": "^3.1.35",
    "kth-node-server": "^3.1.8",
    "kth-node-session": "^2.0.4",
    "kth-style": "7.0.8",
    "mobx": "^6.3.9",
    "mobx-react": "^7.2.1",
    "passport": "^0.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@kth/eslint-config-kth": "^2.0.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "ansi-regex": "^6.0.1",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.5.0",
    "css-loader": "^6.5.1",
    "eslint": "^7.32.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jest-extended": "^1.2.0",
    "mini-css-extract-plugin": "^2.4.5",
    "nodemon": "^2.0.15",
    "null-loader": "^4.0.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "// (prettier)": "configuration version 2020-06-15 - might also reside in .prettierrc.json",
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "// (babel)": "configuration version 2020-10-21 (WEB) - might also reside in .babelrc.json",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "// (jest)": "configuration version 2020-06-15 (WEB) - might also reside as module.exports in jest.config.js",
  "jest": {
    "clearMocks": true,
    "notifyMode": "failure-change",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "testEnvironment": "jsdom",
    "verbose": true
  }
}
